<p id="notice"><%= notice %></p>

<h1>Listing Bills</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Expenseid</th>
      <th>Day month</th>
      <th>Month</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bills.each do |bill| %>
      <tr>
        <td><%= bill.userid %></td>
        <td><%= bill.expenseid %></td>
        <td><%= bill.day_month %></td>
        <td><%= bill.month %></td>
        <td><%= bill.year %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
