<p id="notice"><%= notice %></p>

<h1>Listing Expenses</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Expense Type</th>
      <th>Frequency</th>
      <th>Projvalue</th>
      <th>Actvalue</th>
      <th>Percent</th>
      <th>Month</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td><%= expense.expense_category_name%></td>
        <td><%= expense.expensetype %></td>
        <td><%= expense.frequency %></td>
        <td><%= expense.projvalue %></td>
        <td><%= expense.actvalue %></td>
        <td><%= expense.percent %></td>
        <td><%= expense.month %></td>
        <td><%= expense.year %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<button onclick="goBack()">Back</button>

<script>
function goBack(){
  window.history.back();
}
</script>
