<p id="notice"><%= notice %></p>

<h1>Listing Incomes</h1>

<table>
  <thead>
    <tr>
      <th>User_id</th>
      <th>Income</th>
      <th>Income Type</th>
      <th>Value</th>
      <th>Frequency</th>
      <th>Day rec</th>
      <th>Month</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.user_id %></td>
        <td><%= income.income %></td>
        <td><%= income.incometype %></td>
        <td><%= income.value %></td>
        <td><%= income.frequency %></td>
        <td><%= income.day_rec %></td>
        <td><%= income.month %></td>
        <td><%= income.year %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
