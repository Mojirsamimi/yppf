<head>
  <script src="http://code.highcharts.com/highcharts.js"></script>
</head>


<h1>Compare</h1>

<div class = "row">
  <div class = "col-md-6">
    <%= bar_chart @month1 %>
    <%= bar_chart @month2 %>

  </div>
  <div class="col-md-6">
    <%=pie_chart @month1%>
    <%=pie_chart @month2%>
  </div>
  <br>
</div>

selectedtypes = <%=@selectedtypes.to_s%>
<div id="container" style="width:100%; height:400px;"></div>

<button onclick="goBack()">Back</button>

<script>
function goBack(){
  window.history.back();
}
</script>
<% $hihi = ['idfj', 'sdfs'] %>
<script>
$(function () {
    var testarr = []
    var testtwo = ['sf', 'sdfs']
    for (i = 0; i < testtwo.length; i++) {
        testarr.push(testtwo[i]);
    }
    $('#container').highcharts({
        chart: {
            type: 'column'
        },
        title: {
            text: 'Expense Comparison'
        },
        xAxis: {
            categories: testarr
        },
        yAxis: {
            title: {
                text: 'Fruit eaten'
                // y axis - how much money
            }
        },
        series: [{
            // months
            name: 'Jane',
            // data: [1, 0, 4, 1, 0, 4, 1, 0, 4, 1, 0]
            data: [1, 0]
        }, {
            name: 'John',
            data: [1, 0]
            // data: [1, 0, 4, 1, 0, 4, 1, 0, 4, 1, 0]
        }]
    });
});
</script>
