<p id="notice"><%= notice %></p>

<h1>Listing Incomes</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Income</th>
      <th>Type</th>
      <th>Value</th>
      <th>Frequency</th>
      <th>Day rec</th>
      <th>Month</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.userid %></td>
        <td><%= income.income %></td>
        <td><%= income.type %></td>
        <td><%= income.value %></td>
        <td><%= income.frequency %></td>
        <td><%= income.day_rec %></td>
        <td><%= income.month %></td>
        <td><%= income.year %></td>
        <td><%= link_to 'Show', income %></td>
        <td><%= link_to 'Edit', edit_income_path(income) %></td>
        <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', new_income_path %>
